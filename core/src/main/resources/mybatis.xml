<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="logImpl" value="SLF4J"/>
    </settings>

    <typeAliases>
        <package name="renthelper.core.model"/>
        <package name="renthelper.core.handlers"/>
    </typeAliases>
    
    <typeHandlers>
        <typeHandler handler="DateTypeHandler" javaType="java.util.Date"/>
        <typeHandler handler="DateTimeTypeHandler" javaType="java.util.Date"/>
    </typeHandlers>
    
</configuration>